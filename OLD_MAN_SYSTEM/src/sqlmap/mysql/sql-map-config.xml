<?xml version="1.0" encoding="utf-8" ?> 
<!DOCTYPE sqlMapConfig PUBLIC "-//iBATIS.com//DTD SQL Map Config 2.0//EN" "http://www.ibatis.com/dtd/sql-map-config-2.dtd" >
<sqlMapConfig>


	<!--定义iBATIS自身应用信息 -->
	<settings cacheModelsEnabled="true" enhancementEnabled="true"
		lazyLoadingEnabled="true" maxRequests="200" maxSessions="100"
		maxTransactions="20" useStatementNamespaces="true" />

	
	<!-- 用户登录-->	
	<sqlMap resource="sqlmap/mysql/pmbs/USER_INFO.xml" />
	<sqlMap resource="sqlmap/mysql/pmbs/ACTIVITY.xml" />
	<sqlMap resource="sqlmap/mysql/pmbs/CUSTOMER.xml" />
	<sqlMap resource="sqlmap/mysql/pmbs/CUSTOMER_HEALTH.xml" />
	<sqlMap resource="sqlmap/mysql/pmbs/EMPLOYEE.xml" />
	<sqlMap resource="sqlmap/mysql/pmbs/FEE.xml" />
	<sqlMap resource="sqlmap/mysql/pmbs/FOOD.xml" />
</sqlMapConfig>


